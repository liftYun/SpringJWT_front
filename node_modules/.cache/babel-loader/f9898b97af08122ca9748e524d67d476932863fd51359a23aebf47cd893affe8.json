{"ast":null,"code":"import { isLoggedIn, getUsername, logout } from '../services/auth';\nimport { ref } from 'vue';\nexport default {\n  setup() {\n    const logged = ref(isLoggedIn());\n    const username = ref(getUsername());\n    const handleLogout = () => {\n      logout();\n      // 페이지 리로드 또는 라우트 강제 이동\n      window.location.href = '/';\n    };\n    return {\n      isLoggedIn: logged,\n      username,\n      handleLogout\n    };\n  }\n};","map":{"version":3,"names":["isLoggedIn","getUsername","logout","ref","setup","logged","username","handleLogout","window","location","href"],"sources":["/Users/idoyun/Ssafy/SpringJWT_front/springjwt_vue/src/components/HomeView.vue"],"sourcesContent":["<template>\n    <div>\n      <div v-if=\"!isLoggedIn\">\n        <button @click=\"$router.push('/login')\">로그인</button>\n        <button @click=\"$router.push('/signup')\">회원가입</button>\n      </div>\n      <div v-else>\n        {{ username }}님 로그인 중\n        <button @click=\"handleLogout\">로그아웃</button>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  import { isLoggedIn, getUsername, logout } from '../services/auth';\n  import { ref } from 'vue';\n  \n  export default {\n    setup() {\n      const logged = ref(isLoggedIn());\n      const username = ref(getUsername());\n      const handleLogout = () => {\n        logout();\n        // 페이지 리로드 또는 라우트 강제 이동\n        window.location.href = '/';\n      };\n      return { isLoggedIn: logged, username, handleLogout };\n    }\n  };\n  </script>"],"mappings":"AAcE,SAASA,UAAU,EAAEC,WAAW,EAAEC,MAAK,QAAS,kBAAkB;AAClE,SAASC,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIF,GAAG,CAACH,UAAU,CAAC,CAAC,CAAC;IAChC,MAAMM,QAAO,GAAIH,GAAG,CAACF,WAAW,CAAC,CAAC,CAAC;IACnC,MAAMM,YAAW,GAAIA,CAAA,KAAM;MACzBL,MAAM,CAAC,CAAC;MACR;MACAM,MAAM,CAACC,QAAQ,CAACC,IAAG,GAAI,GAAG;IAC5B,CAAC;IACD,OAAO;MAAEV,UAAU,EAAEK,MAAM;MAAEC,QAAQ;MAAEC;IAAa,CAAC;EACvD;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDepen